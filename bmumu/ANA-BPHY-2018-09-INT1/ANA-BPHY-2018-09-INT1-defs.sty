%%% for additional material and comments
%
\newboolean{extramaterial}
% 
% include extra material or not?
% \setboolean{extramaterial}{true}
\setboolean{extramaterial}{false}
%
\newcommand{\addmaterial}[1]{\protect 
  \ifthenelse{\boolean{extramaterial}}{#1}{}}

\newcommand{\csccomment}[1]{\protect 
  \ifthenelse{\boolean{extramaterial}}{{\em#1}}{}}
%
%%% for comments to convenors
%
